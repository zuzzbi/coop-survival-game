<Window x:Class="CoopSurvivalGame.Game"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoopSurvivalGame"
        mc:Ignorable="d"
        Name="window" Title="Game" Height="450" Width="800">
    <Grid Margin="1,0,-1.4,0">
        <Canvas Focusable="True" Name="canvas" KeyDown="onkeydown">
            <Rectangle Focusable="True" Name="player1" KeyDown="onkeydown" Fill="#FFF4F4F5" Height="41" Stroke="Black" Width="42"/>
            <Rectangle Focusable="True" Name="player2" KeyDown="onkeydown" Fill="#FFF4F4F5" Height="41" Stroke="Black" Width="42"/>
        </Canvas>

    </Grid>
</Window>
